                        <TextBlock Text="В" Grid.Column="2" Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Center" Visibility="{Binding TestType, Converter={StaticResource TT2V}, ConverterParameter=1}"/>
                        <Label Name="labelBvtVdrmResult1" Grid.Column="4" Grid.Row="0" Content="0" HorizontalContentAlignment="Right" FontSize="{StaticResource FontSize30}" Visibility="{Binding TestType, Converter={StaticResource TT2V}, ConverterParameter=1}"
                       Height="Auto" Margin="8,0,-6,0"/>
                        <Label Name="labelBvtVdrmResult2" Grid.Column="6" Grid.Row="0" Content="0" HorizontalContentAlignment="Right" FontSize="{StaticResource FontSize30}" Visibility="{Binding TestType, Converter={StaticResource TT2V}, ConverterParameter=1}"
                       Height="Auto" Margin="8,0,0,0"/>

                        <customControls:LabelWithIndex x:Name="labelWithIndexBvtIdrmResult1" Grid.Column="0" Grid.Row="1"
